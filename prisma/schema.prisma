generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model File {
  id String @id @default(cuid())

  bucket String
  fileName String
  fileType String
  originalName String
  size Float
  url String?

  createdAt DateTime @default(now())
}

model Sequence {
  id String @id @default(cuid())

  active Boolean
  json String?

  createdAt DateTime @default(now())
}